{
  "name": "gatsby-using-graphcms",
  "description": "Gatsby using GraphCMS example",
  "version": "1.0.61",
  "author": "Scott Spence <spences10apps@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "1.10.0",
    "dotenv": "6.2.0",
    "gatsby": "2.0.64",
    "gatsby-image": "2.0.22",
    "gatsby-plugin-manifest": "2.0.11",
    "gatsby-plugin-offline": "2.0.19",
    "gatsby-plugin-react-helmet": "3.0.4",
    "gatsby-plugin-sharp": "2.0.15",
    "gatsby-plugin-styled-components": "3.0.3",
    "gatsby-source-filesystem": "2.0.11",
    "gatsby-source-graphql": "2.0.7",
    "gatsby-transformer-sharp": "2.1.9",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-helmet": "5.2.0",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf gatsby-graphcms/ && rm -rf public/",
    "build": "gatsby build && mv public gatsby-graphcms",
    "dev": "npm run clean && gatsby develop",
    "format": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "now-start": "serve -s ./gatsby-graphcms"
  }
}
