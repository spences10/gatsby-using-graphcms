{
  "name": "gatsby-using-graphcms",
  "description": "Gatsby using GraphCMS example",
  "version": "1.0.45",
  "author": "Scott Spence <spences10apps@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "1.9.2",
    "dotenv": "6.1.0",
    "gatsby": "2.0.62",
    "gatsby-image": "2.0.22",
    "gatsby-plugin-favicon": "3.1.4",
    "gatsby-plugin-google-analytics": "2.0.8",
    "gatsby-plugin-react-helmet": "3.0.4",
    "gatsby-plugin-sharp": "2.0.14",
    "gatsby-plugin-styled-components": "3.0.4",
    "gatsby-source-filesystem": "2.0.10",
    "gatsby-source-graphql": "2.0.7",
    "gatsby-transformer-sharp": "2.1.9",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-helmet": "5.2.0",
    "styled-components": "4.1.2"
  },
  "devDependencies": {
    "husky": "1.2.0",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf gatsby-graphcms/ && rm -rf public/",
