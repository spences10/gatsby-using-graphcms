{
  "name": "gatsby-using-graphcms",
  "description": "Gatsby using GraphCMS example",
  "version": "1.0.430",
  "author": "Scott Spence <spences10apps@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "1.10.6",
    "dotenv": "8.2.0",
    "gatsby": "2.17.9",
    "gatsby-image": "2.2.30",
    "gatsby-plugin-manifest": "2.2.26",
    "gatsby-plugin-offline": "3.0.18",
    "gatsby-plugin-react-helmet": "3.1.13",
    "gatsby-plugin-sharp": "2.2.36",
    "gatsby-plugin-styled-components": "3.1.11",
    "gatsby-source-filesystem": "2.1.35",
    "gatsby-source-graphql": "2.1.21",
    "gatsby-transformer-sharp": "2.3.2",
    "react": "16.11.0",
